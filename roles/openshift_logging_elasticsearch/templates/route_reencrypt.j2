apiVersion: "v1"
kind: "Route"
metadata:
  name: "{{obj_name}}"
{% if labels is defined%}
  labels:
{% for key, value in labels.iteritems() %}
    {{key}}: {{value}}
{% endfor %}
{% endif %}
spec:
  host: {{ route_host }}
  tls:
    termination: passthrough
  to:
    kind: Service
    name: {{ service_name }}
